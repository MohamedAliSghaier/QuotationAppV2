{% extends 'base.html.twig' %}

{% block title %}PrintingMethod{% endblock %}

{% block body %}
    <h1>PrintingMethod</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ printing_method.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ printing_method.name }}</td>
            </tr>
            <tr>
                <th>Setup_cost</th>
                <td>{{ printing_method.setupCost }}</td>
            </tr>
            <tr>
                <th>Per_color_multiplier</th>
                <td>{{ printing_method.perColorMultiplier }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_printing_method_index') }}">back to list</a>

    <a href="{{ path('app_printing_method_edit', {'id': printing_method.id}) }}">edit</a>

    {{ include('printing_method/_delete_form.html.twig') }}
{% endblock %}
